<%--  Created by IntelliJ IDEA.  User: Nguyen  Date: 12/12/2016  Time: 9:19 SA  To change this template use File | Settings | File Templates.--%><style>    .error {        color: #ff0000;        font-style: italic;        font-weight: bold;    }</style><%@ taglib uri="http://www.springframework.org/tags" prefix="spring" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ page language="java" contentType="text/html; charset=UTF-8"         pageEncoding="UTF-8" %><%@page session="true" %><c:set var="contextPath" value="${pageContext.request.contextPath}"/><div class="breadcrumb">    <div class="container">        <div class="breadcrumb-inner">            <ul class="list-inline list-unstyled">                <li><a href="#">Home</a></li>                <li class='active'>Checkout</li>            </ul>        </div><!-- /.breadcrumb-inner -->    </div><!-- /.container --></div><div class="body-content">    <div class="container">        <div class="sign-in-page">            <div class="row">                <h2>${messages}</h2>                <form:form commandName="invoiceModel" class="register-form outer-top-xs"                           id="checkout"                           action="${contextPath}/confirmCheckout" method="POST">                    <div class="col-md-6 col-sm-6 sign-in">                        <h4>Your Personal Details</h4>                        <div class="form-group">                            <label class="info-title" for="exampleInputEmail1">First Name <span>*</span></label>                            <form:input path="firstName" type="text" name="firstName"                                        placeholder="" class="form-control unicase-form-control text-input"/>                           <form:errors path="firstName" cssClass="error" />                        </div>                        <div class="form-group">                            <label class="info-title" for="exampleInputEmail1">Last Name <span>*</span></label>                            <form:input path="lastName" type="text" name="lastName"                                        placeholder="" class="form-control unicase-form-control text-input"/>                            <form:errors path="lastName" cssClass="error" />                        </div>                        <div class="form-group">                            <label class="info-title" for="exampleInputEmail1">Email <span>*</span></label>                            <form:input path="email" type="text" name="email" placeholder=""                                        class="form-control unicase-form-control text-input"/>                            <form:errors path="email" cssClass="error" />                        </div>                        <div class="form-group">                            <label class="info-title" for="exampleInputEmail1">Phone <span>*</span></label>                            <form:input path="phone" type="text" name="phone" placeholder=""                                        class="form-control unicase-form-control text-input"/>                            <form:errors path="phone" cssClass="error" />                        </div>                    </div>                    <div class="col-md-6 col-sm-6 sign-in">                        <h4>Your Address</h4>                        <div class="form-group">                            <label class="info-title" for="exampleInputEmail1">Address <span>*</span></label>                            <form:input path="address" type="text" name="address"                                        placeholder="" class="form-control unicase-form-control text-input"/>                            <form:errors path="address" cssClass="error" />                        </div>                        <div class="form-group">                            <label class="info-title" for="exampleInputEmail1">City <span>*</span></label>                            <form:input path="city" type="text" name="city" placeholder=""                                        class="form-control unicase-form-control text-input"/>                            <form:errors path="city" cssClass="error" />                        </div>                    </div>                    <input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}"/>                    <button onclick="confirmCheckout()"                            type="submit" class="btn-upper btn btn-primary checkout-page-button">Confirm</button>                </form:form>            </div>        </div>    </div></div><script>    function confirmCheckout() {        confirm("Are you sure ?");    }</script>